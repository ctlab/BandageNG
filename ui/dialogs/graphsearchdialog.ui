   <item row="4" column="2">
    <widget class="QWidget" name="blastQueriesWidget2" native="true">
     <layout class="QHBoxLayout" name="horizontalLayout_2">
      <property name="leftMargin">
       <number>0</number>
      </property>
      <property name="topMargin">
       <number>0</number>
